{
  "source": {
    "include": ["main.js", "README.md"]
  },
  "opts": {
    "template": "node_modules/minami",
    "destination": "./docs/",
    "encoding": "utf8"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": [
    "plugins/markdown"
  ]
}